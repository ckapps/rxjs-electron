{
  "name": "@ckapp/rxjs-electron",
  "description": "A collection of RxJS extensions for electron",
  "version": "0.1.1",
  "license": "MIT",
  "author": "Chris",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean:build": "shx rm -rf ./lib",
    "compile": "tsc -b ./tsconfig.build.json",
    "compile:minimal": "tsc -b ./tsconfig.fast.build.json",
    "build:fast": "npm-run-all compile:minimal",
    "build:package": "npm-run-all compile",
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ckapp/math": "^0.0.47",
    "@ckapp/rxjs-snafu": "0.1.0"
  },
  "peerDependencies": {
    "electron": "~16.0.0",
    "rxjs": "~6.6.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "electron": "~16.0.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "rxjs": "~6.6.7",
    "semantic-release": "^19.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "files": [
    "**/*.d.ts",
    "lib/**/!(*.tsbuildinfo)",
    "lib/types/**/!(*.tsbuildinfo)",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/ckapps/rxjs-electron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckapps/rxjs-electron.git"
  },
  "keywords": [
    "rxjs",
    "electron"
  ]
}
