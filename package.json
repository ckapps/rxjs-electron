{
  "name": "@ckapp/rxjs-electron",
  "description": "A collection of RxJS extensions for electron",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Chris",
  "main": "./lib/cjs/index.js",
  "types": "./lib/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build:clean": "shx rm -rf ./lib",
    "compile": "tsc -b ./src/tsconfig.cjs.json",
    "build:declarations": "tsc -b ./src/tsconfig.types.json",
    "build:fast": "npm-run-all build:clean compile",
    "build:package": "npm-run-all build:clean compile build:declarations build:artefacts",
    "build:artefacts": "npm-run-all copy:files copy:declarations",
    "lint": "eslint .",
    "copy:files": "cp -a ./lib/. .",
    "copy:declarations": "cp -a ./lib/. .",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "test": "jest",
    "test:coverage": "npm test -- --coverage"
  },
  "dependencies": {
    "@ckapp/math": "^0.0.14",
    "@ckapp/rxjs-snafu": "0.0.24"
  },
  "peerDependencies": {
    "electron": "^10.1.5",
    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "electron": "^10.1.5",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rxjs": "^6.6.6",
    "shx": "^0.3.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.5.1",
    "tslib": "^2.2.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "**/*.d.ts",
    "lib/**/!(*.tsbuildinfo)",
    "lib/types/**/!(*.tsbuildinfo)",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/ckapps/rxjs-electron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckapps/rxjs-electron.git"
  },
  "keywords": [
    "rxjs"
  ]
}
