{
  "name": "@ckapp/rxjs-electron",
  "description": "A collection of RxJS extensions for electron",
  "version": "0.1.3",
  "license": "MIT",
  "author": "Chris",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean:build": "shx rm -rf ./lib",
    "compile": "tsc -b ./tsconfig.build.json",
    "compile:minimal": "tsc -b ./tsconfig.fast.build.json",
    "build:fast": "npm-run-all compile:minimal",
    "build:package": "npm-run-all compile",
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ckapp/math": "^0.0.57",
    "@ckapp/rxjs-snafu": "0.1.9",
    "tslib": "^2.5.0"
  },
  "peerDependencies": {
    "electron": "^23.0.0",
    "rxjs": "^7.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "electron": "~23.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rxjs": "~7.8.0",
    "semantic-release": "^21.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "files": [
    "**/*.d.ts",
    "lib/**/!(*.tsbuildinfo)",
    "lib/types/**/!(*.tsbuildinfo)",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/ckapps/rxjs-electron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckapps/rxjs-electron.git"
  },
  "keywords": [
    "rxjs",
    "electron"
  ]
}
