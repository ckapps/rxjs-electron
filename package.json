{
  "name": "@ckapp/rxjs-electron",
  "description": "A collection of RxJS extensions for electron",
  "version": "0.0.7",
  "license": "MIT",
  "author": "Chris",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean:build": "shx rm -rf ./lib",
    "compile": "tsc -b ./tsconfig.build.json",
    "compile:minimal": "tsc -b ./tsconfig.fast.build.json",
    "build:fast": "npm-run-all compile:minimal",
    "build:package": "npm-run-all compile",
    "lint": "eslint .",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ckapp/math": "^0.0.39",
    "@ckapp/rxjs-snafu": "0.0.44"
  },
  "peerDependencies": {
    "electron": "~16.0.0",
    "rxjs": "~6.6.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "electron": "~16.0.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rxjs": "~6.6.6",
    "shx": "^0.3.3",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "files": [
    "**/*.d.ts",
    "lib/**/!(*.tsbuildinfo)",
    "lib/types/**/!(*.tsbuildinfo)",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/ckapps/rxjs-electron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckapps/rxjs-electron.git"
  },
  "keywords": [
    "rxjs",
    "electron"
  ]
}
